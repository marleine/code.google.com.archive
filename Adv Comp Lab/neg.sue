proc ICON_neg args {
  icon_setup $args {{origin {0 0}} {orient R0} {name {}} {M {}} {dpc {}}}
  icon_term -type input -origin {-90 -10} -name {A[31:0]}
  icon_term -type output -origin {110 -10} -name out
  icon_property -origin {-40 140} -type user -name name
  icon_property -origin {-40 160} -type user -name M
  icon_property -origin {-40 180} -type user -name dpc
  icon_line -90 -40 -90 30 110 30 110 -40 -90 -40
  icon_property -origin {-80 -10} -label {A[31:0]}
  icon_property -origin {70 -10} -label out
  icon_property -origin {-10 20} -label {Neg - Nor}
}

